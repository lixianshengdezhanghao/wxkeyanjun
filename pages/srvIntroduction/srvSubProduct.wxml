<view class='page'>
  <view class="page__hd">
    <view class="container">
      <text class="title">{{title}}</text>
    </view>
  </view>
  <view wx:for="{{subType}}" class="weui-cells weui-cells_after-title ">
    <navigator url="./srvIntroDetail?title={{title}}&subtitle={{item}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd text-my">{{item}}</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
  </view>

</view>