.input-number{
  border: 1rpx solid #CDC9C9;
 /* margin: 30rpx 40rpx; */
 padding: 0 20rpx;
 font-size: 28rpx;
 box-sizing: border-box;
  /* width: 60%; */
  height: 60rpx;
  border-radius: 8rpx;
  align-items: center;
  margin-top:10rpx;
  margin-left:10rpx;
}
radio .wx-radio-input{
   border-radius: 50%;
   width: 20rpx;
   height: 20rpx;
}
radio .wx-radio-input.wx-radio-input-checked{
   border: none;
   background: #87CEEB;
}
radio .wx-radio-input.wx-radio-input-checked::before{
   border-radius: 50%;
   width: 20rpx;
   height: 20rpx;
    line-height: 40rpx; 
   text-align: center;
   font-size:30rpx;
   color:#fff;
   background: transparent;
    transform:translate(-50%, -50%) scale(1); 
    -webkit-transform:translate(-50%, -50%) scale(1);  
}

